<%
  const siteBaseUrl = ((typeof baseUrl !== 'undefined' && baseUrl) ? baseUrl : (process.env.APP_URL || 'https://sweethome.com')).replace(/\/$/, '');
  const pageUrl = `${siteBaseUrl}/properties-for-sale-dubai`;
  const pageDesc = (typeof pageMetaDescription !== 'undefined' && pageMetaDescription) ? pageMetaDescription : 'Find properties for sale in Dubai, UAE. Browse apartments, villas and off-plan. Expert real estate guidance from Sweet Home.';
  const pageTitle = (typeof title !== 'undefined' && title) ? title : 'Properties for Sale in Dubai';
%>
<meta name="description" content="<%= pageDesc %>">
<!-- Canonical and hreflang are output by main layout when canonicalUrl is passed -->

<!-- Open Graph -->
<meta property="og:type" content="website">
<meta property="og:title" content="<%= pageTitle %> | Sweet Home">
<meta property="og:description" content="<%= pageDesc %>">
<meta property="og:url" content="<%= (typeof canonicalUrl !== 'undefined' && canonicalUrl) ? canonicalUrl : pageUrl %>">
<meta property="og:image" content="<%= siteBaseUrl %>/images/dubai-hero.jpg">
<meta property="og:image:alt" content="Properties for sale in Dubai, UAE – Sweet Home real estate">
<meta property="og:site_name" content="Sweet Home">
<meta property="og:locale" content="en_US">

<!-- Twitter Card -->
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="<%= pageTitle %> | Sweet Home">
<meta name="twitter:description" content="<%= pageDesc %>">
<meta name="twitter:image" content="<%= siteBaseUrl %>/images/dubai-hero.jpg">
<meta name="twitter:image:alt" content="Properties for sale in Dubai, UAE – Sweet Home real estate">

<!-- JSON-LD WebPage + BreadcrumbList -->
<script type="application/ld+json"><%- (typeof minifyJSON !== 'undefined' ? minifyJSON({
  '@context': 'https://schema.org',
  '@graph': [
    {
      '@type': 'WebPage',
      '@id': pageUrl + '#webpage',
      name: pageTitle + ' | Sweet Home',
      description: pageDesc,
      url: (typeof canonicalUrl !== 'undefined' && canonicalUrl) ? canonicalUrl : pageUrl,
      isPartOf: { '@type': 'WebSite', '@id': siteBaseUrl + '/#website', name: 'Sweet Home' }
    },
    {
      '@type': 'BreadcrumbList',
      itemListElement: [
        { '@type': 'ListItem', position: 1, name: 'Home', item: siteBaseUrl + '/' },
        { '@type': 'ListItem', position: 2, name: pageTitle, item: (typeof canonicalUrl !== 'undefined' && canonicalUrl) ? canonicalUrl : pageUrl }
      ]
    }
  ]
}) : JSON.stringify({
  '@context': 'https://schema.org',
  '@graph': [
    {
      '@type': 'WebPage',
      '@id': pageUrl + '#webpage',
      name: pageTitle + ' | Sweet Home',
      description: pageDesc,
      url: (typeof canonicalUrl !== 'undefined' && canonicalUrl) ? canonicalUrl : pageUrl,
      isPartOf: { '@type': 'WebSite', '@id': siteBaseUrl + '/#website', name: 'Sweet Home' }
    },
    {
      '@type': 'BreadcrumbList',
      itemListElement: [
        { '@type': 'ListItem', position: 1, name: 'Home', item: siteBaseUrl + '/' },
        { '@type': 'ListItem', position: 2, name: pageTitle, item: (typeof canonicalUrl !== 'undefined' && canonicalUrl) ? canonicalUrl : pageUrl }
      ]
    }
  ]
})) %></script>

<!-- JSON-LD FAQPage (Dubai) -->
<script type="application/ld+json"><%- (typeof minifyJSON !== 'undefined' ? minifyJSON({
  '@context': 'https://schema.org',
  '@type': 'FAQPage',
  mainEntity: [
    { '@type': 'Question', name: 'Why invest in Dubai real estate?', acceptedAnswer: { '@type': 'Answer', text: 'Dubai\'s residential market reached record levels in Q3 2025, with population over four million, strong UAE GDP growth (4.9%), and an influx of high net worth individuals. Tax-free ecosystem, ease of mortgages, and Golden Visa support long-term demand. Savills ranked Dubai top for attracting individual wealth.' } },
    { '@type': 'Question', name: 'How active is the Dubai residential market?', acceptedAnswer: { '@type': 'Answer', text: 'Q3 2025 maintained transaction volumes over 50,000. Apartments accounted for 86% of transactions; off-plan represented 69% of deals. Average quarterly volumes rose from 22,750 (2022) to 49,500 YTD 2025. Ready market had over 16,500 transactions in Q3.' } },
    { '@type': 'Question', name: 'Where are the most active areas in Dubai?', acceptedAnswer: { '@type': 'Answer', text: 'Zone 6 (37%): JVC, Dubailand, Damac Hills 2, The Valley, Damac Lagoons. Zone 3 (29%): JBR, Dubai Marina, Emirates Living, JLT, Barsha. Zone 2 (13%): Business Bay, Downtown, City Walk.' } },
    { '@type': 'Question', name: 'What is the supply and completion outlook for Dubai?', acceptedAnswer: { '@type': 'Answer', text: 'Approx. 8,500 units added in Q3 2025; YTD nearly 30,000. Around 10,000 units slated for Q4 2025. Over 250,000 units anticipated by end of 2028. Over 10,000 units launched in Q3, 97% apartments.' } },
    { '@type': 'Question', name: 'Is now a good time to buy property in Dubai?', acceptedAnswer: { '@type': 'Answer', text: 'Q3 2025 delivered record metrics. Population forecast to reach 5 million by 2030. Political stability and business-friendly ecosystem support ongoing investment. Henley & Partners anticipate 9,800 millionaires migrating to the UAE in 2025. Sweet Home can help with expert guidance for buying or investing in Dubai.' } }
  ]
}) : JSON.stringify({
  '@context': 'https://schema.org',
  '@type': 'FAQPage',
  mainEntity: [
    { '@type': 'Question', name: 'Why invest in Dubai real estate?', acceptedAnswer: { '@type': 'Answer', text: 'Dubai\'s residential market reached record levels in Q3 2025. Tax-free ecosystem, Golden Visa, and quality of life support long-term demand. Savills ranked Dubai top for attracting individual wealth.' } },
    { '@type': 'Question', name: 'How active is the Dubai residential market?', acceptedAnswer: { '@type': 'Answer', text: 'Transaction volumes over 50,000 in Q3 2025. Apartments 86%; off-plan 69%. Volumes rose from 22,750 (2022) to 49,500 YTD 2025.' } },
    { '@type': 'Question', name: 'Where are the most active areas in Dubai?', acceptedAnswer: { '@type': 'Answer', text: 'Zone 6 (37%): JVC, Dubailand, Damac Hills 2. Zone 3 (29%): JBR, Dubai Marina, JLT. Zone 2 (13%): Business Bay, Downtown, City Walk.' } },
    { '@type': 'Question', name: 'What is the supply and completion outlook for Dubai?', acceptedAnswer: { '@type': 'Answer', text: 'YTD 2025 nearly 30,000 completions; 10,000 slated Q4. Over 250,000 units by end of 2028.' } },
    { '@type': 'Question', name: 'Is now a good time to buy property in Dubai?', acceptedAnswer: { '@type': 'Answer', text: 'Record Q3 2025 metrics. Population forecast 5 million by 2030. Sweet Home can help with expert guidance for buying or investing in Dubai.' } }
  ]
})) %></script>

<!-- JSON-LD ItemList for recommended properties -->
<% if (typeof recommendedProperties !== 'undefined' && recommendedProperties && recommendedProperties.length > 0) { %>
<script type="application/ld+json"><%- (typeof minifyJSON !== 'undefined' ? minifyJSON({
  '@context': 'https://schema.org',
  '@type': 'ItemList',
  name: 'Properties for Sale in Dubai',
  description: pageDesc,
  itemListElement: recommendedProperties.map((p, idx) => ({
    '@type': 'ListItem',
    position: idx + 1,
    item: {
      '@type': 'RealEstateListing',
      name: p.title,
      url: `${siteBaseUrl}/properties/${p.slug}`,
      address: {
        '@type': 'PostalAddress',
        addressLocality: p.city || 'Dubai',
        addressCountry: p.country || 'UAE'
      }
    }
  }))
}) : JSON.stringify({
  '@context': 'https://schema.org',
  '@type': 'ItemList',
  name: 'Properties for Sale in Dubai',
  description: pageDesc,
  itemListElement: recommendedProperties.map((p, idx) => ({
    '@type': 'ListItem',
    position: idx + 1,
    item: {
      '@type': 'RealEstateListing',
      name: p.title,
      url: `${siteBaseUrl}/properties/${p.slug}`,
      address: {
        '@type': 'PostalAddress',
        addressLocality: p.city || 'Dubai',
        addressCountry: p.country || 'UAE'
      }
    }
  }))
})) %></script>
<% } %>
