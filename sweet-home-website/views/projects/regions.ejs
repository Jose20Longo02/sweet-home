<% title = t('projects.regions.title','Projects by Region'); useDarkHeader = false; useMainContainer = false; hideFooter = true; %>
<link rel="stylesheet" href="/css/projects-regions.css" />

<main class="regions-page" aria-label="<%= t('projects.regions.aria.page','Projects by region') %>">
  <section class="regions-split">
    <a class="region region-left" href="/projects?country=Cyprus" aria-label="<%= t('projects.regions.aria.cyprus','See all projects in Cyprus') %>" tabindex="0">
      <div class="region-media" aria-hidden="true">
        <!-- REPLACE: cyprus BG -->
      </div>
      <div class="region-overlay"></div>
      <h1 class="region-title"><%= t('projects.regions.cyprusTitle','Projects in Cyprus') %></h1>
      <span class="region-cta" aria-hidden="false"><%= t('projects.regions.cta','See all projects') %></span>
    </a>
    <a class="region region-right" href="/projects?country=UAE&city=Dubai" aria-label="<%= t('projects.regions.aria.dubai','See all projects in Dubai') %>" tabindex="0">
      <div class="region-media" aria-hidden="true">
        <!-- REPLACE: dubai BG -->
      </div>
      <div class="region-overlay"></div>
      <h2 class="region-title"><%= t('projects.regions.dubaiTitle','Projects in Dubai') %></h2>
      <span class="region-cta" aria-hidden="false"><%= t('projects.regions.cta','See all projects') %></span>
    </a>
    <div class="regions-divider" aria-hidden="true"></div>
  </section>
</main>

